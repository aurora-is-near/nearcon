{
  "name": "nearcon",
  "version": "0.1.0",
  "license": "(MIT AND Apache-2.0)",
  "private": true,
  "scripts": {
    "build": "env-cmd -e mainnet -r .config.js parcel build src/index.html --public-url ./",
    "build_testnet": "env-cmd -e development -r .config.js parcel build src/index.html --public-url ./",
    "start": "env-cmd -e mainnet -r .config.js parcel src/index.html --port 1111",
    "start_testnet": "env-cmd -e development -r .config.js parcel src/index.html --port 2222",
    "lint": "eslint src"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-asset-copier": "^1.1.0",
    "parcel-transformer-google-fonts": "^0.1.3"
  },
  "dependencies": {
    "ethers": "^5.4.6",
    "web3modal": "^1.9.4"
  },
  "browserslist": [
    "since 2017-04"
  ],
  "assetsPath": "src/img",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurora-is-near/nearcon"
  },
  "parcel": {
    "transforms": {
      "*.html": [
        "parcel-transformer-google-fonts"
      ]
    }
  }
}
